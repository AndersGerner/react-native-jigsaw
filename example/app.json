{
  "expo": {
    "name": "JigsawPlayground",
    "description": "JigsawPlayground",
    "slug": "jigsaw-playground",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.draftbit.jigsaw",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.draftbit.jigsaw",
      "versionCode": 1
    },
    "packagerOpts": {
      "assetExts": ["ttf"],
      "config": "./metro.config.js",
      "projectRoots": ""
    }
  }
}
